{
    "name": "Vandy Class Finder",
    "version": "1.0",
    "description": "Determines the concentration of a class on YES",
    "manifest_version": 2,
    "background": {
        "scripts": [ "background.js" ]
    },
    "content_scripts": [
        {
          "matches": [ "<all_urls>" ],
          "js": [ "jquery-3.3.1.min.js", "content.js" ]
        }
    ],
    "browser_action": {
        "default_icon": "vanderbilt.png"
    }
}